<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    @Scripts.Render("~/bundles/jquery")

</head>
<body style="overflow-x:hidden; background-image:url('/Content/sconexus-allback1.png')">
    <div style="position:absolute; top:0px; left:0px; background-image:url('/Content/sconexus-back5.png'); width:100%; height:209px;">

    </div>
    <div style="position:absolute; top:0px; left:0px; width:209px; height:192px; background-image:url('/Content/sconexus5.jpg');"></div>
    <div class="navbar navbar-inverse navbar-fixed-top">

        <div id="MenuView" class="container-fluid">

        </div>

    </div>
    <nav id="leftNavId" class="navbar navbar-default navbar-fixed-left navbar-left-index" style="width:100%; background-color:rgba(0, 0, 0, 0.2); margin-top:180px; min-width:35%; overflow:visible" role="navigation">

        

        <div id="ControlsView" class="container-fluid" style="overflow-x:auto">

            @Html.Partial("ControlsView")

        </div>


    </nav>

    @RenderBody()

    @Scripts.Render("~/bundles/scripts")

</body>
</html>
